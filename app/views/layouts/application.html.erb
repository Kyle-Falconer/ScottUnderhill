	<meta name=viewport content="width=device-width, initial-scale=1">
	<title>Scott Underhill</title>
	<%= stylesheet_link_tag    'application', media: 'all' %>
	<%= javascript_include_tag 'application'%>
	<%= csrf_meta_tags %>
